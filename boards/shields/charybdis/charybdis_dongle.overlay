#include "charybdis.dtsi"

&charybdis_5col_layout {
    transform = <&charybdis_5col_layout>;
};

&charybdis_6col_layout {
    transform = <&charybdis_6col_layout>;
};

/ {
    chosen {
        zmk,kscan = &mock_kscan;
        zmk,physical-layout = &charybdis_5col_layout;
    };

    mock_kscan: kscan_1 {
        compatible = "zmk,kscan-mock";
        columns = <0>;
        rows = <0>;
        events = <0>;
    };
};